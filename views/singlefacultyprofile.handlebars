

<img  id="image" src="{{profile.imageSrc}}" alt="" class="img-thumbnail">

<form>
  <div class="form-group row">
    <label for="inputName" class="col-sm-2 col-form-label"> Name : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="inputName" value="{{profile.full_name}}">
    </div>
  </div>
    <div class="form-group row">
    <label for="inputEmail" class="col-sm-2 col-form-label"> Email id : </label>
    <div class="col-sm-10">
      <input type="email" class="form-control" id="inputEmail" value={{profile.email}}>
    </div>
  </div>
    <div class="form-group row">
    <label for="inputQualification" class="col-sm-2 col-form-label"> Qualification : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="inputName" value="{{profile.qualification}}">
    </div>
  </div>
    <div class="form-group row">
    <label for="inputName" class="col-sm-2 col-form-label"> Current Position : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="inputName" value="{{profile.current_position}}">
    </div>
  </div>
      <div class="form-group row">
    <label for="inputQualification" class="col-sm-2 col-form-label"> Bio : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="inputName" value="{{profile.Bio}}">
    </div>
  </div>
        <div class="form-group row">
    <label for="inputAddress" class="col-sm-2 col-form-label"> Address : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="inputAddress" value="{{profile.address}}">
    </div>
  </div>

</form>
  {{> footer}}
<script src="/facultyprofile"></script>