<h1>Faculty Profile</h1>

  <form class="form new-profile-form">
  <div class="row mt-4">
  <div class="col-md-6">
    <h3>Create your faculty profile: </h3>

 
  <div class="form-group row">
    <label for="new-name" class="col-sm-2 col-form-label"> Name : </label>
    <div class="col-sm-10">
      <input  type="text" class="form-control" id="new-name" name="new-name" />
    </div>
  </div>
    <div class="form-group row">
    <label for="new-email" class="col-sm-2 col-form-label"> Email id : </label>
    <div class="col-sm-10">
      <input type="email" class="form-control" id="new-email" name="new-email">
    </div>
  </div>
    <div class="form-group row">
    <label for="new-qualification" class="col-sm-2 col-form-label"> Qualification : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="new-qualification" name="new-qualification">
    </div>
  </div>
    <div class="form-group row">
    <label for="new-current-position" class="col-sm-2 col-form-label"> Current Position : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="new-current-position" name="new-current-position">
    </div>
  </div>
      <div class="form-group row">
    <label for="new-bio" class="col-sm-2 col-form-label"> Bio : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="new-bio" name="new-bio">
    </div>
  </div>
      <div class="form-group row">
    <label for="new-address" class="col-sm-2 col-form-label"> Address : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="new-address" name="new-address">
    </div>
  </div>


  <div class="form-group">
        <button type="submit" class="btn btn-primary">Create</button>
      </div>
 
  </div>
  </div>
  </form>
    {{> footer}}
 <script src="/facultyprofile"></script>



      