


<form>
  <div class="form-group row">
    <label for="studentName" class="col-sm-2 col-form-label"> Name : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="studentName" value="{{profile.full_name}}">
    </div>
  </div>
    <div class="form-group row">
    <label for="studentEmail" class="col-sm-2 col-form-label"> Email id : </label>
    <div class="col-sm-10">
      <input type="email" class="form-control" id="studentEmail" value={{profile.email}}>
    </div>
  </div>
    <div class="form-group row">
    <label for="studentAddress" class="col-sm-2 col-form-label"> Address : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="studentAddress" value="{{profile.address}}">
    </div>
  </div>
    <div class="form-group row">
    <label for="studentParent" class="col-sm-2 col-form-label"> Parent/Guardians name : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="studentParent" value="{{profile.parent_name}}">
    </div>
  </div>
      <div class="form-group row">
    <label for="studentGrades" class="col-sm-2 col-form-label"> Grades : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="studentGrades" value="{{profile.grades}}">
    </div>
  </div>
        <div class="form-group row">
    <label for="attendance" class="col-sm-2 col-form-label"> Attendance % : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="attendance" value="{{profile.classes_missed}}">
    </div>
  </div>

</form>
   {{> footer}}
<script src="/studentprofile"></script>