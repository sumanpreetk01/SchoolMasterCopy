<h1>Student Profile</h1>

  <form class="form new-student-profile-form">
  <div class="row mt-4">
  <div class="col-md-6">
    <h3>Create your student profile: </h3>

 
  <div class="form-group row">
    <label for="new-student-name" class="col-sm-2 col-form-label"> Name : </label>
    <div class="col-sm-10">
      <input  type="text" class="form-control" id="new-student-name" name="new-name" />
    </div>
  </div>
    <div class="form-group row">
    <label for="new-student-email" class="col-sm-2 col-form-label"> Email id : </label>
    <div class="col-sm-10">
      <input type="email" class="form-control" id="new-student-email" name="new-email">
    </div>
  </div>
    <div class="form-group row">
    <label for="new-student-address" class="col-sm-2 col-form-label"> Address : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="new-student-address" name="new-address">
    </div>
  </div>
    <div class="form-group row">
    <label for="new-student-parent-name" class="col-sm-2 col-form-label"> Parent Name : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="new-student-parent-name" name="new-parent-name">
    </div>
  </div>
      <div class="form-group row">
    <label for="new-grades" class="col-sm-2 col-form-label"> Grades : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="new-grades" name="new-grades">
    </div>
  </div>
      <div class="form-group row">
    <label for="new-attendance" class="col-sm-2 col-form-label"> Attendance % : </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="new-attendance" name="new-attendance">
    </div>
  </div>

  <div class="form-group">
        <button type="submit" class="btn btn-primary">Create</button>
      </div>
    
  </div>
  </div>
  </form>
     {{> footer}}
 <script src="/studentprofile.js"></script>



      